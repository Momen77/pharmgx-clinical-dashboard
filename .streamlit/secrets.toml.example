# Copy this file to .streamlit/secrets.toml and fill in your API keys
# Note: Never commit actual secrets to version control

[api_keys]
# Get from https://www.ncbi.nlm.nih.gov/account/
ncbi_email = "your-email@example.com"
ncbi_api_key = "your-ncbi-api-key"

# Get from https://www.bioontology.org/accounts/new
bioportal_api_key = "your-bioportal-api-key"

# Optional: Add other API keys here
# pharmgkb_api_key = "your-pharmgkb-api-key"

